// printed no.of nodes in the given linked list 192111582//
#include <stdio.h>
#include <stdlib.h>
struct Node {
    int data;
    struct Node *next;
};
void insert(struct Node **head, int data) {
    struct Node *newNode = (struct Node *)malloc(sizeof(struct Node));
    newNode->data = data;
    newNode->next = NULL;

    if (*head == NULL) {
        *head = newNode;
    } else {
        struct Node *temp = *head;
        while (temp->next != NULL) {
            temp = temp->next;
        }
        temp->next = newNode;
    }
}
int countNodes(struct Node *head) {
    int count = 0;
    struct Node *current = head;
    
    while (current != NULL) {
        count++;
        current = current->next;
    }
    
    return count;
}

int main() {
    struct Node *list = NULL;
    insert(&list, 10);
    insert(&list, 20);
    insert(&list, 30);
    insert(&list, 40);
    insert(&list, 50);
    
    printf("Linked List: ");
    struct Node *current = list;
    while (current != NULL) {
        printf("%d -> ", current->data);
        current = current->next;
    }
    printf("NULL\n");
    
    int nodeCount = countNodes(list);
    printf("Number of nodes in the linked list: %d\n", nodeCount);
    
    return 0;
}
